{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": ["s3:GetObject"],
      "Resource": [
        "arn:aws:s3:::fourthstack-data/synthetic/*",
        "arn:aws:s3:::fourthstack-data/infused/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": ["s3:PutObject"],
      "Resource": ["arn:aws:s3:::fourthstack-data/reports/*"]
    }
  ]
}
